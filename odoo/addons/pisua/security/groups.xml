<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record model="ir.module.category" id="module_category_pisua">
            <field name="name">Gesti√≥n de Pisos (Pisukide)</field>
            <field name="description">Niveles de acceso para la app de Pisos</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_pisua_user" model="res.groups">
            <field name="name">Laravel Inquilino (Usuario)</field>
            <field name="category_id" ref="module_category_pisua"/>
        </record>

        <record id="group_pisua_manager" model="res.groups">
            <field name="name">Laravel Coordinador</field>
            <field name="category_id" ref="module_category_pisua"/>
            <field name="implied_ids" eval="[(4, ref('group_pisua_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>